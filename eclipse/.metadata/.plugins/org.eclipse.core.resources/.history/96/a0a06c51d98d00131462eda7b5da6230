package com.nitrogenegames.netcraft.net;

import java.util.ArrayList;

import net.minecraft.world.World;

public interface INetBlock { //THIS CLASS IS A WIP
	ArrayList getConnected(World par1, int par2, int par3, int par4); //USED TO GET CONNECTED BLOCKS
	int[] getCore(World par1, int par2, int par3, int par4); //RETURNS CORE'S COORDINATES WHERE int[0] is the x, int[1] is y, and int[2] is z
	NetEntity getEntity(World par1, int par2, int par3, int par4);
}
